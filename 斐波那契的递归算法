#include <stdio.h>
#include <windows.h>
#include<string.h>
#include <math.h>
#include<windows.h>
int Fac1(int n)
{
	if (n <= 2)
	{
		return 1;
	}
	else
	{
		return Fac1(n-2)+ Fac1(n - 1);
	}
}
int main()
{
	int n = 0;
	while (1)
	{
		scanf("%d", &n);
		int i = Fac1(n);
		printf("%d\n", i);
	}
	return 0;
}
